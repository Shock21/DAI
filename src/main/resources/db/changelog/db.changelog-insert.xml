<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="Tiberiu" id="T-1.3" labels="insert">
        <preConditions>
            <sqlCheck expectedResult="3">
                SELECT COUNT(*)
                FROM databasechangelog
                WHERE ID in ('V-1.0', 'V-1.1', 'V-1.2');
            </sqlCheck>
        </preConditions>
        <tagDatabase tag="T-1.3"/>
    </changeSet>

    <changeSet author="Tiberiu" id="V-1.3" labels="insert">
        <insert tableName="Artist">
            <column name="name" type="VARCHAR(32)" value="Artist1"/>
        </insert>

        <insert tableName="Artist">
            <column name="name" type="VARCHAR(32)" value="Artist2"/>
        </insert>

        <insert tableName="Song">
            <column name="name" type="VARCHAR(32)" value="Song1"/>
            <column name="artist_id" type="INT" value="1"/>
            <column name="location" type="VARCHAR(256)" value="location"/>
        </insert>

        <insert tableName="Song">
            <column name="name" type="VARCHAR(32)" value="Song2"/>
            <column name="artist_id" type="INT" value="2"/>
            <column name="location" type="VARCHAR(256)" value="location"/>
        </insert>

        <rollback>
            <delete tableName="Song"/>
            <delete tableName="Artist"/>
        </rollback>
    </changeSet>
</databaseChangeLog>