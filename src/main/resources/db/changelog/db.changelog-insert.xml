<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="me" id="T-1.3" labels="insert">
        <preConditions>
            <sqlCheck expectedResult="3">
                SELECT COUNT(*)
                FROM databasechangelog
                WHERE ID in ('V-1.0', 'V-1.1', 'V-1.2');
            </sqlCheck>
        </preConditions>
        <tagDatabase tag="T-1.3"/>
    </changeSet>

    <changeSet author="me" id="V-1.3" labels="insert">
        <insert tableName="identity">
            <column name="identity_id" type="NUMBER(9,0)" value="1"/>
            <column name="namespace" type="VARCHAR(32)" value="Strategic"/>
            <column name="type" type="VARCHAR(32)" value="Customer"/>
            <column name="instance" type="VARCHAR(256)" value="EMAGCO"/>
        </insert>

        <insert tableName="identity">
            <column name="identity_id" type="NUMBER(9,0)" value="2"/>
            <column name="namespace" type="VARCHAR(32)" value="WPG"/>
            <column name="type" type="VARCHAR(32)" value="Customer"/>
            <column name="instance" type="VARCHAR(256)" value="EMAG"/>
        </insert>
        
        <insert tableName="relationship">
            <column name="relationship_id" type="NUMBER(9,0)" value="1"/>
            <column name="name" type="VARCHAR(32)" value="IS"/>
            <column name="from_identity_id" type="NUMBER(9,0)" value="1"/>
            <column name="to_identity_id" type="NUMBER(9,0)" value="2"/>
        </insert>

        <insert tableName="relationship">
            <column name="relationship_id" type="NUMBER(9,0)" value="2"/>
            <column name="name" type="VARCHAR(32)" value="IS"/>
            <column name="from_identity_id" type="NUMBER(9,0)" value="2"/>
            <column name="to_identity_id" type="NUMBER(9,0)" value="1"/>
        </insert>

        <rollback>
            <delete tableName="relationship"/>
            <delete tableName="identity"/>
        </rollback>
    </changeSet>
</databaseChangeLog>