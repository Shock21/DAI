<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="Tiberiu" id="T-1.2" labels="foreignKeys">
        <preConditions>
            <sqlCheck expectedResult="2">
                SELECT COUNT(*)
                FROM databasechangelog
                WHERE ID in ('V-1.0', 'V-1.1');
            </sqlCheck>
        </preConditions>
        <tagDatabase tag="T-1.2"/>
    </changeSet>

    <changeSet author="Tiberiu" id="V-1.2" labels="foreignKeys">
        <addForeignKeyConstraint baseColumnNames="artist_id"
                                 baseTableName="Song"
                                 constraintName="fk_from_artist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="Artist"/>
        <rollback>
            <dropForeignKeyConstraint baseTableName="relationship" constraintName="fk_from_identity"/>
        </rollback>
    </changeSet>

</databaseChangeLog>